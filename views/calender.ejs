<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="styles/style.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://kit.fontawesome.com/8093dc5ac1.js" crossorigin="anonymous"></script>
    <title>Template</title>
</head>

<body>

    <%- include ('partials/header.ejs') %>
    <%- include ('partials/nav.ejs') %>

    <p id="start">START PAGE</p>

    <%events2.forEach(function(event) {%>

        <div class="eventContainer">
            <div class="card">
                <p class="event-title"><%= event.title %></p>
                <p class="event-time"><%= event.time %></p>
                <button class="edit-button" data-id="<%= event.id %>">Edit</button>
                <button class="delete-button" onclick="handleDelete(<%= event.id %>)">Delete</button>
            </div>
        </div>
        <%console.log(event)%>
        <%})%>

    <%- include ('partials/footer.ejs') %>


    <script src="./js/app.js"></script>


</body>

</html>